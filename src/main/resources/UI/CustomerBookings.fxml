<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="463.0" prefWidth="728.0"
      xmlns="http://javafx.com/javafx/17.0.12"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="UI.CustomerBookingsController">
    <children>
        <Label text="My Bookings">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <TableView fx:id="bookingsTable" prefHeight="200.0" prefWidth="200.0">
            <columns>
                <TableColumn fx:id="bookingNumCol" prefWidth="136.0" text="Booking #" />
                <TableColumn fx:id="originCol" prefWidth="142.0" text="Origin" />
                <TableColumn fx:id="destinationCol" prefWidth="142.0" text="Destination" />
                <TableColumn fx:id="dateCol" prefWidth="141.0" text="Date" />
                <TableColumn fx:id="bookingDateCol" prefWidth="165.0" text="Booking Date" />
            </columns>
        </TableView>
        <HBox prefHeight="100.0" prefWidth="200.0">
            <children>
                <Button fx:id="backButton" mnemonicParsing="false" prefHeight="26.0" prefWidth="143.0" text="Back to Menu" />
                <HBox prefHeight="113.0" prefWidth="484.0" />
                <Button fx:id="cancelButton" mnemonicParsing="false" prefHeight="26.0" prefWidth="120.0" text="Cancel Booking" />
            </children>
        </HBox>
    </children>
</VBox>